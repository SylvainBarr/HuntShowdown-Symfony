<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {% block stylesheets %}
            {{ encore_entry_link_tags('front_styles') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('scripts') }}
        {% endblock %}
    </head>
    <body>
    {% block header %}
        {% include 'front/partials/common/_header.html.twig' %}
    {% endblock %}
    <main>
        {% block body %}
        {% endblock %}
    </main>
    {% block footer %}
        {% include 'front/partials/common/_footer.html.twig' %}
    {% endblock %}
    </body>
</html>
